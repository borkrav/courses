#!/bin/csh

# Run the same program consecutive times. Should run properly each time.
# Requires quite a bit of memory
# Forces lots of tlb faults.
sys161 kernel "p newtests/vm-data3; p newtests/vm-data3; p newtests/vm-data3; p newtests/vm-data3; p newtests/vm-data3; p newtests/vm-data3; p newtests/vm-data3; p newtests/vm-data3; p newtests/vm-data3; p newtests/vm-data3; p newtests/vm-stack1; p newtests/vm-stack2; p newtests/vm-data1; p newtests/vm-data1; p newtests/vm-data1; p newtests/vm-data1; p newtests/vm-data1; p newtests/vm-data1; p newtests/vm-data1; p newtests/vm-data1; p newtests/vm-data1; p newtests/vm-data1; p newtests/vm-crash1; p newtests/vm-crash2; p newtests/vm-crash3; q"
